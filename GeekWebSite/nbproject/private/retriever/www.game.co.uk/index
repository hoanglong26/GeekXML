						<div class="ad" id="ad_TopNavEspot">
							
				            <div class="textSpot">	          	
								<nav class="newPlatformSplitNav">
	<section class="accountBox">
		<a class="shop" href="#">Shop</a><a class="login" href="#">Log In</a>
