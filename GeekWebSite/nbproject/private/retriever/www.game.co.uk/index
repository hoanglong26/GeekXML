				if (typeof Salmon == "undefined") {
					Salmon = {};
					Salmon.Global = {};
					Salmon.Global.CreateCookie = function(name, value, days) {
					  var expires;
					  if (days) {
					    var date = new Date();
					    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
					    expires = "; expires=" + date.toGMTString();
					  } else expires = "";
					  document.cookie = name + "=" + value + expires + "; path=/";
					};
					Salmon.Global.ReadCookie = function(name) {
						var nameEQ = name + "=";
						var ca = document.cookie.split(';');
						for(var i=0;i < ca.length;i++) {
							var c = ca[i];
							while (c.charAt(0)==' ') c = c.substring(1,c.length);
							if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
						}
						return null;
					}
					Salmon.Global.ReadCookieStartsWith = function(name) {
						var nameEQ = name + "=";
						var ca = document.cookie.split(';');
						for(var i=0;i < ca.length;i++) {
							var c = ca[i];
							while (c.charAt(0)==' ') c = c.substring(1,c.length);
							var isMatch = c.match(nameEQ)
							if (isMatch != null) {	
								return c.substring(isMatch[0].length,c.length);
							}
						}
						return null;
					}
				} else {
					Salmon.Global = (typeof Salmon.Global != "undefined") ? Salmon.Global : {}
					Salmon.Global.CreateCookie = function(name, value, days) {
					  var expires;
					  if (days) {
					    var date = new Date();
					    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
					    expires = "; expires=" + date.toGMTString();
					  } else expires = "";
					  document.cookie = name + "=" + value + expires + "; path=/";
					};
					Salmon.Global.ReadCookie = function(name) {
						var nameEQ = name + "=";
						var ca = document.cookie.split(';');
						for(var i=0;i < ca.length;i++) {
							var c = ca[i];
							while (c.charAt(0)==' ') c = c.substring(1,c.length);
							if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
						}
						return null;
					}
					Salmon.Global.ReadCookieStartsWith = function(name) {
						var nameEQ = name + "=";
						var ca = document.cookie.split(';');
						for(var i=0;i < ca.length;i++) {
							var c = ca[i];
							while (c.charAt(0)==' ') c = c.substring(1,c.length);
							var isMatch = c.match(nameEQ)
							if (isMatch != null) {	
								return c.substring(isMatch[0].length,c.length);
							}
						}
						return null;
					}
				}
				var cookieValue = Salmon.Global.ReadCookie("JSESSIONID");
				var userCookieValue = Salmon.Global.ReadCookieStartsWith("WC_USERACTIVITY_[0-9].+");
				var brightTagData = {"sessionID":"cookieValue","userData":{"userID":"-1002","ipAddress":"172.30.0.221"},"pageData":{"pageName":"HomePage","referingURL":"","referingSite":"","referingSearchKeyword":"","internalSearchKeyword":""},"articleData":{"type":"HomePage","articleType":"","genre":"","subGenre":""}};
							brightTagData.sessionID=	cookieValue;
							brightTagData.userData.ipAddress=Salmon.Global.ipaddress;
							if(userCookieValue!=null){
								brightTagData.userData.userID=unescape(userCookieValue).split(',')[0];
							}
							else{
								brightTagData.userData.userID="";
							}
							if(document.referrer!=null){
								brightTagData.pageData.referingURL=document.referrer;
							}else{
								brightTagData.pageData.referingURL="";
							}
				if (typeof pageData != 'undefined') 
					pageData = Object.assign(pageData, brightTagData);
				else
					var pageData = brightTagData;
