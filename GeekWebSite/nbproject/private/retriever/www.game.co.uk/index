        <script type="text/javascript">
            var InitialActions = (function(){
                  xcomregValues = {
                        requestUrl: '/webapp/wcs/stores/servlet/AjaxInitialView?langId=44&storeId=10151',
                        newrelicExtraOn: 'true',
                        newrelicExtrasUrl: '//assets.game.net/js/newrelic_extras.js',
                        tealeafOn: 'true',
                        tealeafUrl: '//assets.game.net/js/tealeaf.js',
                        jsonLdON: 'true',
                        pageDataOn: 'true',
                        fromMobile: 'false',
                        orderId: ''
                    },
                    checkoutServelets = ['AnonYourDetailsView', 'AnonDeliveryView', 'AnonPaymentView', 'ExpressOrderItemDisplay', 'ExpressNewPayment', 'ExpressNewDelivery'],
                    orderConfirmationServelet = 'OrderShippingBillingConfirmationView',
                    checkIfCheckout = function(){
                        var isCheckout = false,
                            url = document.location.pathname;
                        for (var i in checkoutServelets) {
                            if (url.indexOf(checkoutServelets[i]) != -1) {
                               isCheckout = true;
                               break;
                            }
                        }
                        return isCheckout;
                    },
                    checkIfOrderconf = function(){
                        return document.location.pathname.indexOf(orderConfirmationServelet) != -1
                    },
                    XmlRequest = (function(){
                        var xmlhttp = new XMLHttpRequest(),
                            sendRequest = function(url, callback) {
                                _callback = callback;
                                xmlhttp.open("GET", url, true);
                                xmlhttp.send();
                            },
                            _callback;
                        xmlhttp.onreadystatechange = function() {
                            if (xmlhttp.readyState == XMLHttpRequest.DONE ) {
                                if (xmlhttp.status == 200) {
                                    ajaxResponseSuccess(xmlhttp.responseText);
                                }
                            }
                        };
                        return {
                            sendRequest: sendRequest
                        }
                    })(),
                    insertScript = function(scriptUrl){
                        var script = document.createElement('script');
                        script.setAttribute('src',scriptUrl);
                        document.head.appendChild(script);
                    },
                    generatePageData = function(){
                        if (typeof pageData != 'undefined') {
                            pageData.userInfo = ajaxInitialReponse.userInfo;
                            pageData.orderInfo = ajaxInitialReponse.orderInfo;
                        } else {
                            window.pageData = {
                                userInfo: ajaxInitialReponse.userInfo,
                                orderInfo: ajaxInitialReponse.orderInfo
                            }
                        }
                    },
                    JSONLD = (function(){
                        var templates = {
                                user: {
                                    "@context": "http://schema.org/",
                                    "@type": "Person",
                                    "name": "",
                                    "alternateName": ""
                                },
                                order: {
                                    "@context": "http://schema.org",
                                    "@type": "Order",
                                    "merchant": {
                                        "@type": "Organization",
                                        "name": "GAME"
                                    },
                                    "orderNumber": "",
                                    "orderStatus": "Browse",
                                    "orderedItem" : []
                                },
                                orderItem:  {
                                    "@context": "http://schema.org",
                                    "@type": "Product",
                                    "name": "",
                                    "sku": "",
                                    "category": "",
                                    "additionalProperty": ['Genre', 'Sub Genre', 'Franchise', 'Franchise Iteration', 'PEGI', 'Publisher', 'Developer', 'Is PreOrder']
                                    ,
                                    "Offers": [
                                    ]
                                },
                                offer: {
                                    "@context": "http://schema.org",
                                    "@type": "Offer",
                                    "name": "",
                                    "sku": "",
                                    "priceSpecification": {
                                        "@type": "DeliveryChargeSpecification",
                                        "price": "",
                                        "priceCurrency": "GBP"
                                    },
                                    "itemCondition": "",
                                    "price": "",
                                    "priceCurrency": "GBP",
                                    "seller": {
                                        "@type": "Organization",
                                        "name": ""
                                    }
                                },
                                property: {
                                    "@type": "PropertyValue",
                                    "Name": "",
                                    "Value": ""
                                }
                            },
                            _response,
                            generateUserJSONLD = function(){
                                var userJson = templates.user,
                                    userInfo = _response.userInfo,
                                    addressTemplate =  {
                                        "@type": "postalAddress",
                                        "name": ""
                                    }
                                if (typeof userInfo.ipAddress != 'undefined') {
                                    addressTemplate.name = userInfo.ipAddress;
                                    userJson.address = addressTemplate;
                                }
                                if (_response.userInfo.isLoggedIn == "false") {
                                    userJson.name = 'Guest';
                                    userJson.alternateName = 'Guest';
                                } else {
                                    userJson.name = userInfo.userName;
                                    userJson.alternateName = userInfo.userHash;
                                }
                                injectJSON(userJson, 'userJSONLD');
                            },
                            generateOfferJSONLD = function(itemInfo){
                                var offerJson = JSON.parse(JSON.stringify(templates.offer));
                                offerJson.name = itemInfo.productName;
                                offerJson.sku = itemInfo.sku;
                                offerJson.price = itemInfo.price;
                                offerJson.seller.name = itemInfo.seller;
                                offerJson.itemCondition = itemInfo.itemCondition;
                                if (typeof itemInfo.deliveryPrice != 'undefined' )
                                    offerJson.priceSpecification.price = itemInfo.deliveryPrice;
                                else
                                    delete offerJson.priceSpecification;
                                return offerJson;
                            },
                            generateProductJSONLD = function(itemInfo, oneOffer){
                                var itemJson = JSON.parse(JSON.stringify(templates.orderItem)),
                                    properties = [];
                                itemJson.name = itemInfo.productName;
                                itemJson.sku = itemInfo.sku;
                                itemJson.category = itemInfo.category;
                                if (oneOffer) {
                                    itemJson.Offers.push(generateOfferJSONLD(itemInfo))
                                } else {
                                    itemJson.Offers = [];
                                }
                                if (typeof itemInfo.isRelatedTo != 'undefined') {
                                    itemJson.isRelatedTo = itemInfo.isRelatedTo;
                                }
                                for (var i in itemJson.additionalProperty) {
                                    var propertyName = itemJson.additionalProperty[i],
                                        propertyValue =itemInfo[propertyName.toLowerCase().replace(/ /g,'')];
                                    if (typeof propertyValue != 'undefined' && propertyValue.length > 0 && propertyValue != 'TBD' && propertyValue != 'TBC') {
                                        var propertyTemplate = JSON.parse(JSON.stringify(templates.property));
                                        propertyTemplate.Name = propertyName;
                                        propertyTemplate.Value = itemInfo[propertyName.toLowerCase().replace(/ /g,'')];
                                        properties.push(propertyTemplate);
                                    }
                                }
                                itemJson.additionalProperty = properties;
                                return itemJson;
                            },
                            generateOrderJSONLD = function(){
                                var orderJson = templates.order,
                                    orderInfo = _response.orderInfo
                                orderJson.orderNumber = orderInfo.orderNumber;
                                orderJson.orderStatus  = (checkIfCheckout()) ? 'Checkout' : checkIfOrderconf() ? 'Complete' : 'Browse';
                                for (var i in orderInfo.itemsList) {
                                    orderJson.orderedItem.push(generateProductJSONLD(orderInfo.itemsList[i], true));
                                }
                                injectJSON(orderJson, 'orderJSONLD');
                            },
                            generateJSONLDS = function(response){
                                _response = response;
                                if (parseInt(response.orderInfo.orderQuantity) > 0)  generateOrderJSONLD();
                                generateUserJSONLD();
                            },
                            injectJSON = function(json, id){
                                var el = document.createElement('script');
                                el.type = 'application/ld+json';
                                el.text = JSON.stringify(json);
                                el.setAttribute('id', id)
                                document.querySelector('head').appendChild(el);
                            }
                        return {
                            generateJSONLDS: generateJSONLDS,
                            generateProductJSONLD: generateProductJSONLD,
                            generateOfferJSONLD: generateOfferJSONLD,
                            injectJSON: injectJSON
                        }
                    })(),
                    ajaxResponseSuccess = function(response){
                        window.ajaxInitialReponse = JSON.parse(response);
                        if (xcomregValues.pageDataOn == 'true') generatePageData();
                        if (xcomregValues.jsonLdON == 'true') JSONLD.generateJSONLDS(window.ajaxInitialReponse);
                        
                            if (xcomregValues.newrelicExtraOn == 'true') {
                                insertScript(xcomregValues.newrelicExtrasUrl);
                            }
                            if (xcomregValues.tealeafOn == 'true') {
                                insertScript(xcomregValues.tealeafUrl);
                            }
                        
                    },
                    init = (function(){
                        if (xcomregValues.fromMobile == 'true') xcomregValues.requestUrl += '&fromMobile=true';
                        if (checkIfOrderconf()) xcomregValues.requestUrl += '&orderConf=true&orderId=' + xcomregValues.orderId;
                        XmlRequest.sendRequest(xcomregValues.requestUrl, ajaxResponseSuccess);
                    })();
                return {
                    JSONLD: JSONLD,
                    xcomregValues: xcomregValues
                }
          })();
        </script>
        
