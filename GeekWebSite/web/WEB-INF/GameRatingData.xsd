<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="http://www.gamerating.net"
           xmlns="http://www.gamerating.net">
    <xs:element name="gameRating" type="ratingItem"/>
    <xs:complexType name="ratingItem">
        <xs:sequence>
            <xs:element name="id" type="xs:integer"/>
            <xs:element name="score" type="xs:string"/>
            <xs:element name="reviewer" type="xs:string"/>
            <xs:element name="reviewedDate" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
